extends layout

block content
  h2= "Dialer"
  ul
    div(id="wrapper")
      div(id="phone")
        div(id="numberDisplay")
          input(type='tel')
        div(id="dialpad" class="button-3")
          ul
            li(class="first") 1
            li 2
            li(class="last") 3
            li(class="first") 4
            li 5
            li(class="last") 6
            li(class="first") 7
            li 8
            li(class="last") 9
            li(class="first") 0
            li +
        div(id="actions" class="button-3 deactive")
          ul
            li(href="" class="call") Call
            li(href="" class="clear") Clear
  script(src='/javascripts/jquery.js')
  script(src='/javascripts/button-action.js')
